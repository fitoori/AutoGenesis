# $VENV_DIR/.env  (not /etc/default)
# Log file path
LOG_FILE="/var/log/autogenstudio/autogen_setup.log"

# PostgreSQL credentials (script uses these names)
AUTOGEN_DB_USER="autogen_user"
AUTOGEN_DB_NAME="autogen"
AUTOGEN_DB_PASS=""            # blank â†’ script will auto-generate

# Full SQLAlchemy URL (PASS_URI is built in the script)
DATABASE_URL="postgresql+psycopg://$AUTOGEN_DB_USER:$PASS_URI@localhost/$AUTOGEN_DB_NAME"

# Ollama
OLLAMA_API_URL="http://localhost:11434"
OLLAMA_VERSION="latest"

# Flags
DRY_RUN="false"
