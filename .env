# /etc/default/autogenstudio
# Environment configuration for AutoGen Studio

# Log file path
LOG_FILE="/var/log/autogenstudio/autogen_setup.log"

# PostgreSQL
DB_NAME=$AUTOGEN_DB_NAME
DB_USER=$AUTOGEN_DB_USER
DB_PASSWORD=$DB_PASS
AUTOGEN_DB_USER="autogen_user"
AUTOGEN_DB_NAME="autogen"
AUTOGEN_DB_PASS=""  # Leave blank to autogenerate in script
DATABASE_URL=postgresql+psycopg://$AUTOGEN_DB_USER:$PASS_URI@localhost/$AUTOGEN_DB_NAME

# Optional runtime features
OLLAMA_API_URL="${OLLAMA_API_URL:-http://localhost:11434}"
OLLAMA_VERSION="${OLLAMA_VERSION:-latest}"
DRY_RUN="false"

